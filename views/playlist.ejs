<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('./partials/head'); %>
</head>

<body class="text-center">
  <%- include('./partials/nav', {back:true}); %>
  <div class="container">
    <table class="table table-bordered table-striped">
       <thead>
      <tr>
        <th>Name</th>
        <th>Artists </th>
        <th>Album </th>
      </tr>
    </thead>
    <tbody>
      <% tracks.forEach(function(track){ %>
        <tr>
          <td><%= track.name %> </td>
          <td><%= track.artist.join(',') %> </td>
          <td><%= track.album %> </td>
        </tr>
      <% }) %>
    </tbody>
      <table>

  </div>
  <footer class="footer">
    <%- include('./partials/footer'); %>
  </footer>
  <%- include('./partials/scripts'); %>
</body>

</html>
